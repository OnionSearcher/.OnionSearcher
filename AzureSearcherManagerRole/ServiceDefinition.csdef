<?xml version="1.0" encoding="utf-8"?>
<ServiceDefinition name="AzureSearcherManagerRole" xmlns="http://schemas.microsoft.com/ServiceHosting/2008/10/ServiceDefinition" schemaVersion="2015-04.2.6">
  <WorkerRole name="WebSearcherManagerRole" vmsize="ExtraSmall">
    <ConfigurationSettings>
      <Setting name="Microsoft.WindowsAzure.Plugins.Diagnostics.ConnectionString" />
    </ConfigurationSettings>
    <Imports>
      <!-- doesn t seems usefull and raise a lot of error in infra <Import moduleName="Diagnostics" />-->
    </Imports>
    <Startup>
      <!-- Counter creating by c# don t have enouth right even with Runtime executionContext=elevated -->
      <Task commandLine="PerfCounter.cmd" executionContext="elevated" />
    </Startup>
  </WorkerRole>
</ServiceDefinition>